{
  "name": "@dws/muster-repl",
  "version": "6.5.0",
  "description": "Muster REPL",
  "module": "./esm/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "bin": {
    "muster-repl": "./dist/index.js"
  },
  "scripts": {
    "clean": "del-cli ./dist ./esm",
    "prebuild": "yarn clean",
    "prebuild:full": "yarn clean",
    "build": "yarn build:dist",
    "build:full": "yarn build:dist && yarn build:esm",
    "build:dist": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "node dist/index",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "DWS Technology",
  "license": "MIT",
  "dependencies": {
    "@dws/muster": "^6.5.0",
    "@dws/muster-observable": "^6.5.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.6",
    "chalk": "^2.3.0",
    "cli-highlight": "^2.0.0",
    "lodash": "^4.17.11",
    "mz": "^2.7.0",
    "untildify": "^3.0.2",
    "snyk": "^1.116.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/lodash-es": "^4.14.74",
    "@types/mz": "0.0.32",
    "@types/node": "^10.10.0",
    "@types/untildify": "^3.0.0",
    "typescript": "3.2.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "snyk": true
}
